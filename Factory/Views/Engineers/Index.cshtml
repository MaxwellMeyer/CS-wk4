@{
  Layout = "_Layout";
}

@using Factory.Models;
@model List<Factory.Models.Engineer>;

<h1>Our Engineers...</h1>

<hr>

@if (@Model.Count == 0)
{
  <p>No Engineers yet, woopsies!!</p>
}

@foreach (Engineer engineer in Model)
{
  <li>@Html.ActionLink($"{engineer.EngName}", "Details", new { id = engineer.EngineerId })</li>
}

<hr>

<p>@Html.ActionLink("Add a new Engineer!?", "Create")</p>

<hr>