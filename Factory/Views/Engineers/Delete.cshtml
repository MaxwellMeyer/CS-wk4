@{
  Layout = "_Layout";
}
@model Factory.Models.Engineer
<div class="mainCont">
  <div class="head">
    <h1>Delete Engineer!</h1>
  </div>
  <div class="content">
    <p>Are you sure you want to delete poor @Html.DisplayFor(model => model.EngName)?</p>
    @using (Html.BeginForm())
    {
      <input type="submit" value="Delete" class="btn btn-default">
    }
  </div>
  <div class="back">
    <p>@Html.ActionLink("Back to List of Engineers", "Index")</p>
  </div>
</div>